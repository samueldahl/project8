doctype html
html
    head
      title user list
      script(src='script.js')
      style 
    body
      h1(style='color:red') home fo yo broha and his brohas.
      h2 this is yo user view page #{datae}
      a(href='/createuser') create user
      table
        tr
          td name
          td id
          td email
          td age
          td delete user
          td add user
        each user in data
          tr
            td= user.name
            td= user.id
            td= user.email
            td= user.age
            td
              button(onclick='deleteUser(this, \'' + user.id + '\')') delete user
            td
              button(onclick='editUser(\'' + user.id + '\')') edit user
